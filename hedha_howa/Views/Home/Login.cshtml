@model hedha_howa.Models.Contact

@{ Layout = ""; }
@Scripts.Render("~/Scripts/Forms/General/menu")


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>HLI</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    @Styles.Render("~/AdminLTE/plugins/font-awesome/css")
    @Styles.Render("~/AdminLTE/plugins/ionicons/css")
    @Styles.Render("~/AdminLTE/bootstrap/css")
    @Styles.Render("~/AdminLTE/dist/css")
    @Styles.Render("~/AdminLTE/dist/css/skins")

</head>
<body class="hold-transition skin-blue sidebar-mini" style="background-color:darkgray; margin-top:200px">
    <div class="row">
        <div style=" max-width: 500px; margin:auto">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title"  style=" margin-left:215px">Login</h3>
                </div>
                @using (Html.BeginForm("Login", "Home", FormMethod.Post))
                {
    <div class="box-body">
        <div class="form-group">
            <label for="exampleInputEmail1"  style=" margin-left:10px">Email address</label>
            @Html.EditorFor(Model => Model.Email, new { htmlAttributes = new { @class = "form-control" } })
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1"  style=" margin-left:10px">Password</label>
            @Html.EditorFor(Model => Model.password, new { htmlAttributes = new { @class = "form-control", type = "Password" } })
        </div>


        <div class="box-footer">
            <button type="submit" class="btn btn-primary" style="display: block;position: relative;margin-left: auto;margin-right: auto">Submit</button>
        </div>
    </div>  }
            </div>

        </div>
    </div>


    @Scripts.Render("~/AdminLTE/plugins/jquery/js")
    @Scripts.Render("~/AdminLTE/bootstrap/js")
    @Scripts.Render("~/AdminLTE/plugins/slimscroll/js")
    @Scripts.Render("~/AdminLTE/plugins/fastclick/js")
    @Scripts.Render("~/Scripts/Shared/_Layout")
    @Scripts.Render("~/AdminLTE/dist/js")

</body>
</html>
